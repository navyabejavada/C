#include<stdio.h>
#include<string.h>
void main()
{
char pass[20]
printf("Enter your pass word honestly");
scanf("%s",pass);
int len=strlen(pass);
int 1=0,u=0,d=0,s=0;
if(len>7)
{
for(int i=0;i<len;i++)
{
if(isupper(pass[1]))
u++;
else if(islower(pass[i]))
1++;
else if(isdigit(pass[i]))
d++;
else
s++;
}
if(u>0 && 1>0 && s>0 && d>0)
printf("STRONG PASSWORD");
else
printf("weak");
}
else
printf("WEAK DUE TO LESS CHARACTER");
}
